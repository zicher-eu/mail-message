{
  "name": "zicher/mail-message",
  "description": "Provides a layer on top of message object preparation, and processors for css inline and rendering.",
  "homepage": "https://github.com/zicher-eu/mail-message",
  "keywords": [
    "mail",
    "css",
    "swift",
    "twig"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tomasz Kotlarek",
      "email": "tomasz.kotlarek@zicher.eu"
    }
  ],
  "type": "library",
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "twig/twig": "^2.0",
    "swiftmailer/swiftmailer": "^6.0",
    "tijsverkoyen/css-to-inline-styles": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Zicher\\MailMessage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "": "tests/fixtures/mail/"
    }
  }
}
